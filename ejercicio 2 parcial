#include <iostream>
using namespace std;
int main()
{
	float precio, descuento, precioFinal;

	cout << "introduzca el precio: ";
	cin >> precio;
	cout << "introduzca el porcentaje de descueto: ";
	cin >> descuento;
	precioFinal = precio-(precio * (descuento/100));
	cout << "el precio final con descuento es de : " << precioFinal << endl;
	return 0;












}
