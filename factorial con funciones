#include <iostream>
using namespace std;
//prototipos 
int factorial(int numero);
int main()
{
	int f, n;
	do {
		cout << "ingresar el numero n: ";
		cin >> n;
	} while (n < 0);
	cout << "el facotrial de " << n << " es: " << factorial(n) << endl;
	return 0;
}
int factorial(int n)
{
	float x;
	x = 1;
	for (int i = 1; i <= n; i++)
	{
		x=x* i;
	}
	return x;
}
