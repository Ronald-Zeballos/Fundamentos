#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double B, resultado;
    float x;
    cout << "Ingresar el valor de B: ";
    cin >> B;
    
    x = pow(pow(2, 8 * B), 1.0 / 7); // Usar 1.0 para obtener una división en punto flotante
    cout << "El resultado de la ecuación es: " << x << endl;
    
    return 0;
}
